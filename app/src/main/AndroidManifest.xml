<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abeer.inventoryapp1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CatalogActivity"
            android:label="Inventory">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InsertActivity"
            android:parentActivityName=".ProductDetails">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ProductDetails" />
        </activity>
        <provider
            android:name=".data.InventoryProvider"
            android:authorities="com.example.abeer.inventoryapp1"
            android:exported="false" />
        <activity
            android:name=".ProductDetails"
            android:label="Product Details"></activity>
    </application>

</manifest>